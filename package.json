{
  "name": "spotify-analyzer",
  "version": "0.0.1",
  "description": "UI for analyzing Spotify tracks!",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack/webpack.prod.js",
    "start": "webpack serve --open --progress --config webpack/webpack.dev.js",
    "lint": "eslint --ext .jsx,.js src/",
    "lint-fix": "eslint --ext .jsx,.js --fix src/",
    "test": "jest",
    "test-watch": "jest --watch-all",
    "coverage": "jest --collect-coverage",
    "test-ci": "jest --ci",
    "snapshot": "jest --update-snapshot",
    "format": "prettier --write \"**/*.+(js|jsx|json|css)\""
  },
  "pre-commit": [
    "prettier-fix",
    "lint-fix",
    "test"
  ],
  "author": "Robert Kuhlmann",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@testing-library/react": "^11.1.0",
    "babel-jest": "^26.6.1",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.1",
    "jest-styled-components": "^7.0.3",
    "mini-css-extract-plugin": "^1.2.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.1.2",
    "react-test-renderer": "^17.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.3.1",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1"
  }
}
